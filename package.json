{
  "name": "mitm-life-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check --typescript",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write . --plugin prettier-plugin-astro",
    "format:check": "prettier --check . --plugin prettier-plugin-astro",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "audit": "pnpm audit --audit-level moderate",
    "audit:fix": "pnpm audit --fix",
    "feeds:generate": "echo 'RSS generation via @astrojs/rss integration'",
    "update-deps": "node scripts/update-dependencies.js",
    "health-check": "node scripts/health-check.js",
    "lighthouse": "lhci autorun",
    "bundle-analyze": "node scripts/bundle-analyze.js",
    "content-check": "node scripts/content-validation.js",
    "link-check": "find . -name '*.md' -not -path './node_modules/*' -not -path './.git/*' | head -10 | xargs -I {} markdown-link-check {}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.6",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/tailwind": "^5.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "astro": "^5.14.3",
    "astro-loader-obsidian": "^0.9.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.2",
    "tailwind-merge": "^2.5.4",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.34.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-core": "^4.10.3",
    "broken-link-checker": "^0.7.8",
    "eslint": "^9.34.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "htmlhint": "^1.6.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.2",
    "markdown-link-check": "^3.13.7",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.14",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
