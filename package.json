{
  "name": "mitm-life-blog",
  "version": "1.0.0",
  "description": "OSINT and cybersecurity research blog with monetization features",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "contentlayer build && next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "feeds:generate": "tsx src/lib/feeds.ts",
    "new:post": "tsx scripts/new-post.ts",
    "build:analyze": "ANALYZE=true pnpm build",
    "build:production": "NODE_ENV=production pnpm build",
    "search:index": "pagefind --site out --glob \"**/*.{html,htm}\" --output-subdir pagefind",
    "preview": "pnpm build && pnpm start",
    "clean": "rm -rf .next out .contentlayer",
    "setup": "pnpm install && pnpm build"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@vercel/og": "^0.6.2",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "contentlayer": "^0.3.4",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.378.0",
    "next": "^14.2.0",
    "next-contentlayer": "^0.3.4",
    "next-safe": "^3.4.0",
    "postcss": "^8.4.38",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "shiki": "^1.6.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pagefind": "^1.1.0",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}